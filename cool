-- Hitbox Button
local hitboxButton = Instance.new("TextButton")
hitboxButton.Size = UDim2.new(0, 125, 0, 50)
hitboxButton.Position = UDim2.new(0, 430, 0, 130)
hitboxButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
hitboxButton.Text = "Hitbox"
hitboxButton.Parent = Frame

local hitboxSizeInput = Instance.new("TextBox")
hitboxSizeInput.Size = UDim2.new(0, 125, 0, 30)
hitboxSizeInput.Position = UDim2.new(0, 430, 0, 190) -- Поле ввода под кнопкой
hitboxSizeInput.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
hitboxSizeInput.TextColor3 = Color3.fromRGB(0, 0, 0)
hitboxSizeInput.PlaceholderText = "Hitbox size"
hitboxSizeInput.Text = ""
hitboxSizeInput.Parent = Frame

local hitboxEnabled = false
local defaultHitboxSize = Vector3.new(5, 5, 5) -- Стандартный увеличенный размер хитбокса

local function parseHitboxSize(input)
    local size = tonumber(input)
    if size and size > 0 then
        return Vector3.new(size, size, size)
    else
        return defaultHitboxSize -- Если ввод некорректен, возвращаем стандартный размер
    end
end

hitboxButton.MouseButton1Click:Connect(function()
    hitboxEnabled = not hitboxEnabled
    hitboxButton.BackgroundColor3 = hitboxEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)

    local hitboxSize = parseHitboxSize(hitboxSizeInput.Text)

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= localPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local hrp = player.Character.HumanoidRootPart
            if hitboxEnabled then
                hrp.Size = hitboxSize
                hrp.Transparency = 0.5 -- Сделать хитбокс видимым
                hrp.Material = Enum.Material.ForceField
                hrp.CanCollide = false -- Отключить столкновения
            else
                hrp.Size = Vector3.new(2, 2, 1) -- Стандартный размер
                hrp.Transparency = 1 -- Скрыть хитбокс
                hrp.Material = Enum.Material.Plastic
            end
        end
    end
end)



-- Hitbox Button
local hitboxButton = Instance.new("TextButton")
hitboxButton.Size = UDim2.new(0, 125, 0, 50)
hitboxButton.Position = UDim2.new(0, 10, 0, 310)
hitboxButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
hitboxButton.Text = "Hitbox"
hitboxButton.Parent = Frame

local hitboxEnabled = false
local hitboxSizeInput = Instance.new("TextBox")
hitboxSizeInput.Size = UDim2.new(0, 125, 0, 50)
hitboxSizeInput.Position = UDim2.new(0, 150, 0, 310)
hitboxSizeInput.PlaceholderText = "Size (X,Y,Z)"
hitboxSizeInput.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
hitboxSizeInput.Parent = Frame

local function parseHitboxSize(text)
    local size = text:split(",")
    return Vector3.new(tonumber(size[1]) or 2, tonumber(size[2]) or 2, tonumber(size[3]) or 1)
end

hitboxButton.MouseButton1Click:Connect(function()
    hitboxEnabled = not hitboxEnabled
    hitboxButton.BackgroundColor3 = hitboxEnabled and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)

    local hitboxSize = parseHitboxSize(hitboxSizeInput.Text)

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= localPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local hrp = player.Character.HumanoidRootPart
            if hitboxEnabled then
                hrp.Size = hitboxSize
                hrp.Transparency = 0.5 -- Сделать хитбокс видимым
                hrp.Material = Enum.Material.ForceField
                hrp.CanCollide = false -- Отключить столкновения
            else
                hrp.Size = Vector3.new(2, 2, 1) -- Стандартный размер
                hrp.Transparency = 1 -- Скрыть хитбокс
                hrp.Material = Enum.Material.Plastic
            end
        end
    end
end)

-- Страница 2 (следующая страница)
local nextPageButton = Instance.new("TextButton")
nextPageButton.Size = UDim2.new(0, 50, 0, 50)
nextPageButton.Position = UDim2.new(1, -60, 1, -60) -- Кнопка в правом нижнем углу
nextPageButton.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
nextPageButton.Text = "->"
nextPageButton.Parent = Frame

local page2Frame = Instance.new("Frame", ScreenGui)
page2Frame.Size = UDim2.new(0, 600, 0, 500)
page2Frame.Position = UDim2.new(0.5, -300, 0.5, -250)
page2Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
page2Frame.Visible = false -- Вторая страница скрыта по умолчанию

local backButton = Instance.new("TextButton")
backButton.Size = UDim2.new(0, 50, 0, 50)
backButton.Position = UDim2.new(1, -60, 1, -60) -- Кнопка на второй странице тоже в правом нижнем углу
backButton.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
backButton.Text = "<-"
backButton.Parent = page2Frame

-- Переход на следующую страницу
nextPageButton.MouseButton1Click:Connect(function()
    Frame.Visible = false -- Скрыть текущую страницу
    page2Frame.Visible = true -- Показать вторую страницу
end)

-- Возврат на первую страницу
backButton.MouseButton1Click:Connect(function()
    page2Frame.Visible = false -- Скрыть вторую страницу
    Frame.Visible = true -- Показать первую страницу
end)


local buttonNames = {
    "CHAOS script",
    "Infinite yield",
    "MM2 Script",
    "FlyGui script",
    "Doors script",
    "BABFT Script",
    "Button 6", "Button 7", "Button 8", "Button 9",
    "Button 10", "Button 11", "Button 12", "Button 13", "Button 14", 
    "Button 15", "Button 16", "Button 17", "Button 18", "Button 19", 
    "Button 20", "Button 21", "Button 22", "Button 23"
}

local buttonScripts = {
    "loadstring(game:HttpGet(('https://raw.githubusercontent.com/Dan41/Scripts/main/%5BCHAOS%5D%20CHAOTIC%20HUB.lua'),true))()",
    "loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()",
    "loadstring(game:HttpGet('https://pastebin.com/raw/e89Mn4Ec'))()",
    "loadstring(game:HttpGet('https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt'))()",
    "loadstring(game:HttpGet('https://raw.githubusercontent.com/KINGHUB01/BlackKing-obf/main/Doors%20Blackking%20And%20BobHub'))()",
    "loadstring(game:HttpGet('https://raw.githubusercontent.com/RunDTM/ZeeroxHub/main/Loader.lua'))()",
    "print('Button 6 pressed')", "print('Button 7 pressed')", "print('Button 8 pressed')", 
    "print('Button 9 pressed')", "print('Button 10 pressed')", "print('Button 11 pressed')", 
    "print('Button 12 pressed')", "print('Button 13 pressed')", "print('Button 14 pressed')", 
    "print('Button 15 pressed')", "print('Button 16 pressed')", "print('Button 17 pressed')", 
    "print('Button 18 pressed')", "print('Button 19 pressed')", "print('Button 20 pressed')", 
    "print('Button 21 pressed')", "print('Button 22 pressed')", "print('Button 23 pressed')"
}

local buttonList = {}

-- Добавляем кнопки на страницу 2
for i, buttonName in ipairs(buttonNames) do
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 125, 0, 50)  -- Размер кнопок
    button.Position = UDim2.new(0, 10 + ((i-1) % 3) * 135, 0, 10 + math.floor((i-1) / 3) * 60) -- Расположение кнопок
    button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    button.Text = buttonName
    button.Parent = page2Frame

    -- Переключение состояния кнопки: выполнение скрипта при нажатии
    button.MouseButton1Click:Connect(function()
        loadstring(buttonScripts[i])()  -- Выполнение скрипта
        button.BackgroundColor3 = Color3.fromRGB(0, 255, 0)  -- Изменение цвета кнопки на зеленый
    end)

    -- Сохраняем кнопки в список
    table.insert(buttonList, button)
end
